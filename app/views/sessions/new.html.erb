<h1>Log in</h1>
<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>

  <div class="log_in">
    <%= f.label :email %>
    <%= f.email_field :email %>

    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>

  <div class="log_in_btn"><%= f.submit "Log in" %></div>
<% end %>
